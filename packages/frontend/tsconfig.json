{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~typings/*": [
                "src/typings/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~keys/*": [
                "src/shared/keys/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~interface/*": [
                "src/shared/interface/*"
            ]
        }
    }
}