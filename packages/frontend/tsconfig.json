{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~keys/*": [
                "src/keys/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~router/*": [
                "src/router/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~typings/*": [
                "src/typings/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~interface/*": [
                "src/shared/interface/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~utils/*": [
                "src/shared/utils/*"
            ]
        }
    }
}tentCasingInFileNames": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"composite": true,
		"types": [
			"vite/client"
		]
	},
	"include": [
		"./src/**/*",
		"./vite.config.ts",
		".eslintrc.cjs"
	],
	"exclude": [
		"./node_modules",
		"test",
		"./dist",
		"**/*spec.ts"
	]
}